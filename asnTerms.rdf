<rdf:RDF xmlns:asn="http://purl.org/ASN/schema/core/" xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:dcterms="http://purl.org/dc/terms/" xmlns:loc="http://www.loc.gov/loc.terms/relators/"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:schema="http://schema.org/">
    <!--
 ====================================================== 
-->
    <!--   BEGIN SCHEMA DESCRIPTION                               -->
    <!--
 ====================================================== 
-->
    <rdf:Description rdf:about="http://purl.org/ASN/schema/core/">
        <dc:title xml:lang="en-US">The Achievement Standards Network Core Properties</dc:title>
        <dc:publisher xml:lang="en-US">University of Washington</dc:publisher>
        <dc:contributor xml:lang="en-US">D2L</dc:contributor>
        <dc:description xml:lang="en-US"> The Achievement Standards Network Core namespace provides
            URIs for many of the terms used in ASN metadata instances. Custom ASN application
            profiles can be achieved through use of properties and classes from other RDF schemas
            and ontologies. Vocabulary terms are declared using RDF Schema language to support RDF
            applications. </dc:description>
        <dc:language xml:lang="en-US">English</dc:language>
        <dcterms:issued rdf:datatype="dcterms:W3CDTF">2004-04-29</dcterms:issued>
        <dcterms:modified rdf:datatype="dcterms:W3CDTF">2019-02-26</dcterms:modified>
    </rdf:Description>
    <!--
 ====================================================== 
-->
    <!--   END SCHEMA DESCRIPTIONN                                -->
    <!--
 ====================================================== 
-->
    <!--
 ====================================================== 
-->
    <!--   BEGIN PROPERTY DEFINITIONS FOR DOCUMENTS               -->
    <!--   AND STATEMENTS                                         -->
    <!--
 ====================================================== 
-->
    <!--   Definition of alignedFrom                              -->
    <rdf:Property rdf:about="http://purl.org/ASN/schema/core/alignFrom">
        <rdfs:label>align from</rdfs:label>
        <rdfs:comment> A standards document or statement from which the statement or document being
            described is aligned. </rdfs:comment>
        <dc:description xml:lang="en-US"> An alignment is an assertion of some degree of eqiuvalency
            between the subject and the object of the assertion. </dc:description>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    </rdf:Property>
    <!--   Definition of alignedTo   -->
    <rdf:Property rdf:about="http://purl.org/ASN/schema/core/alignTo">
        <rdfs:label>align to</rdfs:label>
        <rdfs:comment> A standards document or statement to which the statement or document being
            described is aligned. </rdfs:comment>
        <dc:description xml:lang="en-US"> An alignment is an assertion of some degree of eqiuvalency
            between the subject and the object of the assertion. </dc:description>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    </rdf:Property>
    <!--   Definition of altStatementNotation   -->
    <!--   Added December 3, 2012   -->
    <rdf:Property rdf:about="http://purl.org/ASN/schema/core/altStatementNotation">
        <rdfs:label>Alternative Statement Notation</rdfs:label>
        <rdfs:comment> An alphanumeric notation or ID code identifying the statement in common use
            among end-users. </rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifer"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    </rdf:Property>
    <!--   Definition of authorityStatus   -->
    <!--   Modified November 14, 2007   -->
    <rdf:Property rdf:about="http://purl.org/ASN/schema/core/authorityStatus">
        <rdfs:label>Authority Status</rdfs:label>
        <rdfs:comment> The status of a standards statement denoting whether the statement owes its
            origin to the promulgating agency of the standards document of which the statement is a
            member (i.e., value "Original") or to some 3rd party (i.e., "Derived"). </rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    </rdf:Property>
    <!--   Definition of comment   -->
    <rdf:Property rdf:about="http://purl.org/ASN/schema/core/comment">
        <rdfs:label>comment</rdfs:label>
        <rdfs:comment> Supplemental text provided by the promulgating body that clarifies the
            nature, scope or use of the statement being described. </rdfs:comment>
        <dc:description xml:lang="en-US"> Use comment when the text provides useful context for the
            statement. </dc:description>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    </rdf:Property>
    <!--   Definition of complexityLevel   -->
    <!--   Added June 12, 2017   -->
    <rdf:Property rdf:about="http://purl.org/ASN/schema/core/complexityLevel">
        <rdfs:label>Complexity Level</rdfs:label>
        <rdfs:comment> The expected performance level of a learner or professional as defined by a
            competency. </rdfs:comment>
        <dc:description xml:lang="en-US"> The value of asn:complexityLevel should be a concept IRI
            of a member of the class asn:ProficiencyScale. </dc:description>
        <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
        <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/ProficiencyScale"/>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ASN/schema/core/proficiencyLevel"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    </rdf:Property>
    <!--   Definition of conceptKeyword  -->
    <!--   Added 2009-09-01    -->
    <rdf:Property rdf:about="http://purl.org/ASN/schema/core/conceptKeyword">
        <rdfs:label>Concept Keyword</rdfs:label>
        <rdfs:comment> A word or phrase used by the promulgating agency to refine and differentiate
            individual statements contextually. </rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/elements/1.1/subject"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    </rdf:Property>
    <!--   Definition of conceptTerm  -->
    <!--   Modified November 11, 2006   -->
    <rdf:Property rdf:about="http://purl.org/ASN/schema/core/conceptTerm">
        <rdfs:label>Concept Term</rdfs:label>
        <rdfs:comment> A term from a controlled vocabulary used by the promulgating agency to refine
            and differentiate individual statements contextually. </rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/elements/1.1/subject"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    </rdf:Property>
    <!--   Definition of crossSubjectReference   -->
    <!--   Added June 11, 2009   -->
    <rdf:Property rdf:about="http://purl.org/ASN/schema/core/crossSubjectReference">
        <rdfs:label>Cross-subject Reference</rdfs:label>
        <rdfs:comment> An explicit reference by the promulgating agency establishing a relationship
            between the statement being described and statement in a separate subject standards
            document (interdisciplinary statement references). </rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/elements/1.1/references"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    </rdf:Property>
    <!--   Definition of derivedFrom   -->
    <rdf:Property rdf:about="http://purl.org/ASN/schema/core/derivedFrom">
        <rdfs:label>Derived From</rdfs:label>
        <rdfs:comment> The URI of a statement from which the derived statement being described has
            been derived. </rdfs:comment>
        <dc:description xml:lang="en-US"> The derivedFrom property is used by 3rd parties only in
            derived statements and must appear in conjunction with a value of "Derived" in the
            statement's authorityStatus property. </dc:description>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    </rdf:Property>
    <!--   Definition of exportVersion   -->
    <rdf:Property rdf:about="http://purl.org/ASN/schema/core/exportVersion">
        <rdfs:label>Export Version</rdfs:label>
        <rdfs:comment> The URI of the export version of the standards file. </rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    </rdf:Property>
    <!--   Definition of indexingStatus   -->
    <!--   Modified November 14, 2007   -->
    <rdf:Property rdf:about="http://purl.org/ASN/schema/core/indexingStatus">
        <rdfs:label>Indexing Status</rdfs:label>
        <rdfs:comment> The status of the statement denoting whether correlators should or should not
            assign the statement during correlation--Values="Yes"/"No". </rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    </rdf:Property>
    <!--   Definition of identifier   -->
    <!--   Added 2011-07-16   -->
    <rdf:Property rdf:about="http://purl.org/ASN/schema/core/identifier">
        <rdfs:label>Alternative Identifier</rdfs:label>
        <rdfs:comment> An alternative URI by which the resource is identified. </rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    </rdf:Property>
    <!--   Definition of jurisdiction   -->
    <!--   Modified June 19, 2005  -->
    <rdf:Property rdf:about="http://purl.org/ASN/schema/core/jurisdiction">
        <rdfs:label>Jurisdiction</rdfs:label>
        <rdfs:comment> The jurisdiction or domain of the standard's application--e.g., California,
            Nebraska, etc. </rdfs:comment>
        <dc:description xml:lang="en-US"> The URI of the jurisdiction from the ASNJurisdiction
            controlled vocabulary. </dc:description>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    </rdf:Property>
    <!--   Definition of listID  -->
    <!--   Added November 29, 2012   -->
    <rdf:Property rdf:about="http://purl.org/ASN/schema/core/listID">
        <rdfs:label>List ID</rdfs:label>
        <rdfs:comment> A set of one or more alphanumeric characters and/or symbols denoting the
            positioning of the statement being described in a sequential listing of statements. </rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifer"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    </rdf:Property>
    <!--   Definition of localSubject   -->
    <!--   Added November 11, 2006   -->
    <!--   Modified November 14, 2007   -->
    <rdf:Property rdf:about="http://purl.org/ASN/schema/core/localSubject">
        <rdfs:label>Local Subject</rdfs:label>
        <rdfs:comment> The text string denoting the subject of the statement as designated by the
            promulgating agency. This subject may be different from the asn:subject used for subject
            classification within the ASN. </rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/subject"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    </rdf:Property>
    <!--   Definition of proficiencyLevel   -->
    <!--   Added January 05, 2014   -->
    <rdf:Property rdf:about="http://purl.org/ASN/schema/core/proficiencyLevel">
        <rdfs:label>Proficiency Level</rdfs:label>
        <rdfs:comment xml:lang="en"> The expected performance level of a learner or professional in
            relationship to a competency. </rdfs:comment>
        <dc:description xml:lang="en"> The value of asn:proficiencyLevel should be a concept IRI of
            a member of the class asn:ProficiencyScale. </dc:description>
        <schema:domainIncludes rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
        <schema:domainIncludes rdf:resource="http://purl.org/ASN/schema/core/LearningResource"/>
        <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/ProficiencyScale"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    </rdf:Property>
    <!--   Definition of publicationStatus   -->
    <!--   Added November 14, 2007   -->
    <rdf:Property rdf:about="http://purl.org/ASN/schema/core/publicationStatus">
        <rdfs:label>Publication Status</rdfs:label>
        <rdfs:comment> The publication status of the of the standards document--e.g., Draft,
            Published, Deprecated. </rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    </rdf:Property>
    <!--   Definition of repositoryDate   -->
    <!--   Revised June 19, 2005   -->
    <rdf:Property rdf:about="http://purl.org/ASN/schema/core/repositoryDate">
        <rdfs:label>Repository Date</rdfs:label>
        <rdfs:comment> The date the standards document was added to the repository. </rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    </rdf:Property>
    <!--   Definition of skillEmbodied   -->
    <!--   Added June 11, 2009   -->
    <rdf:Property rdf:about="http://purl.org/ASN/schema/core/skillEmbodied">
        <rdfs:label>Skill Embodied</rdfs:label>
        <rdfs:comment> Cognitive, affective, psychomotor or other skill directly or indirectly
            embodied in the statement. </rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    </rdf:Property>
    <!--   Definition of statementLabel   -->
    <!--   Revised July 12, 2005   -->
    <rdf:Property rdf:about="http://purl.org/ASN/schema/core/statementLabel">
        <rdfs:label>Statement Label</rdfs:label>
        <rdfs:comment> The textual label designating the class of statement in the structure of
            statements in the Standard Document--e.g., Standard, Benchmark, Strand, Topic, etc. </rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    </rdf:Property>
    <!--   Definition of statementNotation  -->
    <!--   Revised June 19, 2005   -->
    <rdf:Property rdf:about="http://purl.org/ASN/schema/core/statementNotation">
        <rdfs:label>Statement Notation</rdfs:label>
        <rdfs:comment> An alphanumeric identifier of a Statement as defined by the promulgating
            agency. Statement notation must be unique within the defining organization's namespace. </rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifer"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    </rdf:Property>
    <!--   Definition of weight  -->
    <!--   Added June 12, 2017   -->
    <rdf:Property rdf:about="http://purl.org/ASN/schema/core/weight">
        <rdfs:label>Weight</rdfs:label>
        <rdfs:comment> A textually asserted measurement of the weight, degree, percent, or strength
            of a recommendation, requirement, or comparison; e.g., "15%", ".5". </rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    </rdf:Property>
    <!--
 ====================================================== 
-->
    <!--
 END PROPERTY DEFINITIONS FOR DOCUMENTS AND STATEMENTS  
-->
