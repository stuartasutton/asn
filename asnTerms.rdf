<!-- ====================================================== -->
<!-- ASN APPLICATION PROFILE DESCRIPTION                    -->
<!--                                                        -->    
<!-- This document defines properties in the ASN namespace  -->
<!-- O N L Y. For this namespace plus properties from       -->
<!-- other namespaces used in ASN descriptions, use the     -->
<!-- the asnProfile.rdf document.                           --> 
<!-- ====================================================== -->    

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/asnTerms.rdf">
    <dc:title xml:lang="en-US">The Achievement Standards Network Core Terms</dc:title>
    <dcterms:description xml:lang="en-US">Achievement Standards Network Core Terms provides URIs and definitions for all of the terms defined in the ASN namespace.</dcterms:description>
    <dcterms:publisher xml:lang="en-US">Information School of the University of Washington (Seattle)</dcterms:publisher>
    <dcterms:contributor xml:lang="en-US">Desire2Learn (D2L)</dcterms:contributor>
    <dcterms:language xml:lang="en-US">English</dcterms:language>
    <dcterms:rights>https://creativecommons.org/licenses/by/4.0/</dcterms:rights>
    <dcterms:rightsHolder xml:lang="en-US">University of Washington, Seattle</dcterms:rightsHolder>
    <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2020-11-28</dcterms:issued>
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2023-01-02</dcterms:modified>
  </rdf:Property>
  
<!-- ====================================================== -->
<!-- CLASSES                                                -->
<!-- ====================================================== -->
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/CompetencyMap">
    <rdfs:comment xml:lang="en-US">A Competency Map can be viewed as an identified graph that aggregates two or more competency statements (asn:statement) and the relationships asserted among them.</rdfs:comment>
    <rdfs:label xml:lang="en-US">CompetencyMap</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <vann:usageNote>A Competency Map is a profile that borrows statements (Statement) from two or more standard documents (StandardDocument).A Competency Map may or may not define new statements in its own namespace.</vann:usageNote>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/CorrelationResource">
    <rdfs:comment xml:lang="en-US">A resource that describes the mapping between a learning resource and a node in a learning outcomes framework.</rdfs:comment>
    <rdfs:label xml:lang="en-US">Correlation Resource</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <vann:usageNote>The Correlation Resource is an entity intended to provide a rich description of a mapping (correlation) between a Learning Resource and a Statement when a binary assertion of a relationship is insufficient.</vann:usageNote>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/CriterionCategory">
    <rdfs:label xml:lang="en-US">Criterion Category</rdfs:label>
    <rdfs:comment xml:lang="en-US">Resource identifying a category by which Rubric Criterion may be grouped.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdfs:Class>  
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/CriterionLevel">
    <rdfs:label xml:lang="en-US">Criterion Level</rdfs:label>
    <rdfs:comment xml:lang="en-US">Resource identifying one or more benchmarks that must be met to achieve a performance task.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/DerivedStandardDocument">
    <rdfs:label xml:lang="en-US">Derived Standard Document</rdfs:label>
    <rdfs:comment xml:lang="en-US">A collection of statements representing an expectation of what students should know, value, and be able to do at a target age or age range that are substantially defined by a different jurisdiction.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/DerivedStatement">
    <rdfs:label xml:lang="en-US">Derived Statement</rdfs:label>
    <rdfs:comment xml:lang="en-US">An assertion representing an expectation of what students should know, value, and be able to do at a target age or age range that is derived from a statement defined by a different jurisdiction.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/EducationalFramework">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:label xml:lang="en-US">Educational Framework</rdfs:label>
    <vann:usageNote xml:lang="en-US">Similar to the class ConceptScheme with an intended focus on education and training. Examples of educational frameworks include, but are not limited to, frameworks enumerating competencies, text complexity, proficiency scales, assessment rubrics of various forms, and specialized subject vocabularies.</vann:usageNote>
    <rdfs:comment xml:lang="en-US">The class of all structured sets of conceptual resources intentionally designed for use as value vocabulary terms in education and training contexts.</rdfs:comment>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/JurisdictionScheme">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:label xml:lang="en-US">Jurisdiction Encoding Schemes</rdfs:label>
    <rdfs:comment xml:lang="en-US">Controlled vocabularies of terms denoting the juristiction or domain of the standards document.</rdfs:comment>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/LearningResource">
    <rdfs:comment xml:lang="en-US">A resource that is used as part of an Educational Activity (e.g. a textbook) or that describes (e.g. a lesson plan) or records the Educational Activity (e.g. an audio- or video-recording of a lesson).</rdfs:comment>
    <rdfs:label xml:lang="en-US">Learning Resource</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/ProficiencyScale">
    <rdfs:label xml:lang="en-US">Proficiency Scale</rdfs:label>
    <rdfs:comment xml:lang="en-US">The class of all structured concept schemes identifying discrete levels of expertise and performance mastery required either by a competency or to be achieved by a learner.</rdfs:comment>
    <dcterms:description xml:lang="en-US">Proficiency scales define levels of performance (what a person does) as distinct from knowledge of specific information (what a person knows) and outline tasks a person can manage and the skills necessary to progressively accomplish explicit competencies at increasing levels of complexity. Proficiency scales: (1) assist in making judgments about the kinds of tasks related to a competency that a person is able to perform; and (2) to compare the abilities of different persons with regard to achievement of those competencies at different levels.</dcterms:description>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/ProgressionLevel">
    <rdfs:label xml:lang="en-US">Progression Level</rdfs:label>
    <rdfs:comment xml:lang="en-US">Is an dentifiable point along a developmental progression of competence, achievement or temporal position.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/ProgressionModel">
    <rdfs:comment xml:lang="en-US">Is a set of identifiable points along a developmental progression including increasing levels of competence, achievement or temporal position (e.g., "Beginner", "Second Quarter").</rdfs:comment>
    <rdfs:label xml:lang="en-US">Progression Model</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/RightsAndAttribution">
    <rdfs:label xml:lang="en-US">Rights and Attribution</rdfs:label>
    <rdfs:comment xml:lang="en-US">Resource providing information about the rights and attribution requirements of the entity making the metadata description available.</rdfs:comment>
    <dcterms:description xml:lang="en-US">These rights are distinct from any rights held in the content of the competency framework or competencies described.</dcterms:description> 
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/Rubric">
    <rdfs:comment xml:lang="en-US">Resource providing guidance in evaluating the quality and level of person's responses or actions based on an explicit set of criteria.</rdfs:comment>
    <rdfs:label xml:lang="en-US">Rubric</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/RubricCriterion">
    <rdfs:label xml:lang="en-US">Rubric Criterion</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:comment xml:lang="en-US">Resource identifying a criterion that must be met to demonstrate quality performance of an objective or task.</rdfs:comment>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/StandardDocument">
    <rdfs:label xml:lang="en-US">Standard Document</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:comment xml:lang="en-US">A collection of statements representing an expectation of what students or workers should know, value, and be able to do.</rdfs:comment>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/Statement">
    <rdfs:label xml:lang="en-US">Statement</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:comment xml:lang="en-US">An assertion representing an expectation of what students or workers should know, value, and be able to do.</rdfs:comment>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/StatusScheme">
    <rdfs:comment xml:lang="en-US">Controlled vocabularies of terms denoting the publication status of the standards document.</rdfs:comment>
    <rdfs:label xml:lang="en-US">Status Encoding Schemes</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdfs:Class>

<!-- ====================================================== -->
<!-- PROPERTIES                                             -->
<!-- ====================================================== -->
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/alignFrom">
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <dcterms:description xml:lang="en-US">An alignment is an assertion of some degree of eqiuvalency between the subject and the object of the assertion.</dcterms:description>
    <rdfs:comment xml:lang="en-US">A standards document or statement from which the statement or document being described is aligned.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:label xml:lang="en-US">align from</rdfs:label>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/alignTo">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:label xml:lang="en-US">align to</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <dcterms:description xml:lang="en-US">An alignment is an assertion of some degree of eqiuvalency between the subject and the object of the assertion.</dcterms:description>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:comment xml:lang="en-US">A standards document or statement to which the statement or document being described is aligned.</rdfs:comment>
  </rdf:Property>
   
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/altIdentifier">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
    <rdfs:label xml:lang="en-US">Alternative Identifier</rdfs:label>
    <rdfs:comment xml:lang="en-US">An alternative URI by which the resource is identified.</rdfs:comment>
  </rdf:Property>
  
 <rdf:Property rdf:about="http://purl.org/ASN/schema/core/altStatementNotation">
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:label xml:lang="en-US">Alternative Statement Notation</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:comment xml:lang="en-US">An alphanumeric notation or ID code identifying the statement in common use among end-users.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifer"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/assessesCompetency">
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/LearningResource"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:label xml:lang="en-US">Assesses Competency</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:comment xml:lang="en-US">The competency that the learning resource being described assesses.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/conformsTo"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/assessmentRubric">
    <rdfs:comment xml:lang="en-US">Related resource providing guidance in evaluating the quality and level of person's responses or actions based on an explicit set of criteria.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:label xml:lang="en-US">Assessment Rubric</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Rubric"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/authorityStatus">
    <rdfs:label xml:lang="en-US">Authority Status</rdfs:label>
    <rdfs:comment xml:lang="en-US">The status of a standards statement denoting whether the statement owes its origin to the promulgating agency of the standards document of which the statement is a member (i.e., value "Original") or to some 3rd party (i.e., "Derived").</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/benchmark">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/CriterionLevel"/>
    <rdfs:comment xml:lang="en-US">Description of a level of achievement on a performance task defined by the RubricCriterion.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"/>
    <vann:usageNote xml:lang="en-US">Can have one or more per CriterionLevel.</vann:usageNote>
    <rdfs:label xml:lang="en-US">Benchmark</rdfs:label>
    <owl:equivalentProperty rdf:resource="https://ceds.ed.gov/CEDSElementDetails.aspx?TermId=14443"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/broadAlignment">
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:label xml:lang="en-US">Broad Alignment</rdfs:label>
    <rdfs:comment xml:lang="en-US">The target competency covers all of the relevant concepts in the competency being described as well as relevant concepts not found in the competency being described.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/broadCorrelation">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:comment xml:lang="en-US">The competency covers all of the relevant concepts in the learning resource being described as well as relevant concepts not found in the learning resource.</rdfs:comment>
    <rdfs:label xml:lang="en-US">Broad Correlation</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/LearningResource"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/conformsTo"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/comment">
    <rdfs:comment xml:lang="en-US">Supplemental text provided by the promulgating body that clarifies the nature, scope or use of the statement being described.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:label xml:lang="en-US">Comment</rdfs:label>
    <dcterms:description xml:lang="en-US">Use comment when the text provides useful context for the statement.</dcterms:description>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/complexityLevel">
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/ProficiencyScale"/>
    <rdfs:comment xml:lang="en-US">The expected performance level of a learner or professional as defined by a competency.</rdfs:comment>
    <dcterms:description xml:lang="en-US">The value of asn:complexityLevel should be a concept IRI of a member of the class asn:ProficiencyScale.</dcterms:description>
    <rdfs:label xml:lang="en-US">Complexity Level</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/ASN/schema/core/proficiencyLevel"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/comprisedOf">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:comment xml:lang="en-US">The resource being described includes, comprehends or encompass, in whole or in part, the meaning, nature or importance of the resource being referenced.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:label xml:lang="en-US">Comprised Of</rdfs:label>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/conceptKeyword">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:comment xml:lang="en-US">A word or phrase used by the promulgating agency to refine and differentiate individual statements contextually.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/elements/1.1/subject"/>
    <rdfs:label xml:lang="en-US">Concept Keyword</rdfs:label>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/conceptTerm">
    <rdfs:label xml:lang="en-US">Concept Term</rdfs:label>
    <rdfs:comment xml:lang="en-US">A term from a controlled vocabulary used by the promulgating agency to refine and differentiate individual statements contextually.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/criterionCategoryOf">
    <rdfs:label xml:lang="en-US">Criterion Category Of</rdfs:label>
    <rdfs:comment xml:lang="en-US">Resource referenced is a Rubric to which this CriterionCategory belongs.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/CriterionCategory"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Rubric"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/criterionFor">
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/RubricCriterion"/>
    <rdfs:label xml:lang="en-US">Criterion For</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Rubric"/>
    <rdfs:comment xml:lang="en-US">Reference to the Rubric to which the Rubric Criterion being described belongs.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/crossSubjectReference">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/references"/>
    <rdfs:label xml:lang="en-US">Cross-subject Reference</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:comment xml:lang="en-US">An explicit reference by the promulgating agency establishing a relationship between the statement being described and statement in a separate subject standards document (interdisciplinary statement references).</rdfs:comment>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/derivedFrom">
    <rdfs:comment xml:lang="en-US">The URI of a statement from which the derived statement being described has been derived.</rdfs:comment>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:label xml:lang="en-US">Derived From</rdfs:label>
    <dcterms:description xml:lang="en-US">The derivedFrom property is used by 3rd parties only in derived statements and must appear in conjunction with a value of "Derived" in the statement's authorityStatus property.</dcterms:description>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/exactAlignment">
    <rdfs:comment xml:lang="en-US">The relevant concepts in both competencies are coextensive.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:label xml:lang="en-US">Exact Alignment</rdfs:label>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/exactCorrelation">
    <rdfs:comment xml:lang="en-US">The relevant concepts in the educational resource and the standard statement are coextensive.</rdfs:comment>
    <rdfs:label xml:lang="en-US">Exact Correlation</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/LearningResource"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/conformsTo"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/exportVersion">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:label xml:lang="en-US">Export Version</rdfs:label>
    <rdfs:comment xml:lang="en-US">The export version of the standards file.</rdfs:comment>
  </rdf:Property>  
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/hasChild">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:label xml:lang="en-US">Has Child</rdfs:label>
    <rdfs:comment xml:lang="en-US">The resource being described is higher in some arbitrary hierarchy than the resource being pointed to by this Relation element.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/hasCriterion">
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/RubricCriterion"/>
    <rdfs:comment xml:lang="en-US">Rubric Criterion referenced by the Rubric that defines a criterion to be met to demonstrate quality in execution in performance of a task or obtaining an objective.</rdfs:comment>
    <rdfs:label xml:lang="en-US">Has Criterion</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Rubric"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/hasCriterionCategory">
    <vann:usageNote xml:lang="en-US">Large rubrics are occasionally divided into logical sections or categories that need to be identified and described.</vann:usageNote>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/CriterionCategory"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Rubric"/>
    <rdfs:comment xml:lang="en-US">Resource referenced by the Rubric that defines categories for clustering logical sets of RubricCriterion.</rdfs:comment>
    <rdfs:label xml:lang="en-US">Has Criterion Category</rdfs:label>
  </rdf:Property>
 
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/hasLevel">
    <rdfs:label xml:lang="en-US">Has Level</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/RubricCriterion"/>
    <owl:equivalentProperty rdf:resource="https://ceds.ed.gov/CEDSElementDetails.aspx?TermId=14443"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/CriterionLevel"/>
    <rdfs:comment xml:lang="en-US">Resource description of a level of performance based on a Rubric Criterion.</rdfs:comment>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/hasProgressionLevel">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:label xml:lang="en-US">Has Progression Level</rdfs:label>
    <rdfs:comment xml:lang="en-US">Reference to a specific point in a progression model.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/ProgressionModel"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/ProgressionLevel"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/hasProgressionModel">
    <rdfs:comment xml:lang="en-US">Reference to a progression model used.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Rubric"/>
    <rdfs:label xml:lang="en-US">Has Progression Model</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/ProgressionModel"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/hasScope">
    <rdfs:comment xml:lang="en-US">Description of what the rubric's creator intended to assess or evaluate.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Rubric"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:label xml:lang="en-US">Has Scope</rdfs:label>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/indexingStatus">
    <rdfs:comment xml:lang="en-US">The status of the statement denoting whether correlators should or should not assign the statement during correlation--Values="Yes"/"No".</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:label xml:lang="en-US">Indexing Status</rdfs:label>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/identifier">
    <rdfs:comment xml:lang="en-US">The URI by which the resource is identified by its creator.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:label xml:lang="en-US">Identifier</rdfs:label>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/isChildOf">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:comment xml:lang="en-US">The statement being described is lower in some arbitrary hierarchy than the statement identified in the asn:isChildOf property. The statement identified is a parent of the statement being described.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:label xml:lang="en-US">Statement Label</rdfs:label>
    <rdfs:range rdf:resource= "http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:range rdf:resource= "http://purl.org/ASN/schema/core/Statement"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/jurisdiction">
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <dcterms:description xml:lang="en-US">The URI of the jurisdiction from the ASNJurisdiction controlled vocabulary.</dcterms:description>
    <rdfs:label>Jurisdiction</rdfs:label>
    <rdfs:comment>The jurisdiction or domain of the standard's application--e.g., California, Nebraska, etc.</rdfs:comment>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/levelFor">
    <rdfs:comment xml:lang="en-US">Reference to the asn:Rubric Criterion to which the asn:Criterion Level being described belongs.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:label xml:lang="en-US">Level For</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/RubricCriterion"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/CriterionLevel"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/listID">
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifer"/>
    <rdfs:comment xml:lang="en-US">A set of one or more alphanumeric characters and/or symbols denoting the positioning of the statement being described in a sequential listing of statements.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:label xml:lang="en-US">List ID</rdfs:label>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/localSubject">
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:comment xml:lang="en-US">The text string denoting the subject of the statement as designated by the promulgating agency. This subject may be different from the asn:subject used for subject classification within the ASN.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"/>
    <rdfs:label xml:lang="en-US">Local Subject</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/subject"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/majorAlignment">
    <rdfs:comment xml:lang="en-US">Major overlap of relevant concepts between the two competencies.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:label xml:lang="en-US">Major Alignment</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/majorCorrelation">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/conformsTo"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/LearningResource"/>
    <rdfs:comment xml:lang="en-US">Major overlap of relevant concepts between the learning resource being described and the competency statement.</rdfs:comment>
    <rdfs:label xml:lang="en-US">Major Correlation</rdfs:label>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/minorAlignment">
    <rdfs:comment xml:lang="en-US">Minor overlap of relevant concepts between the two competencies.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:label xml:lang="en-US">Minor Alignment</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/minorCorrelation">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/conformsTo"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/LearningResource"/>
    <rdfs:comment xml:lang="en-US">Minor overlap of relevant concepts between the learning resource being described and the competency statement.</rdfs:comment>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:label xml:lang="en-US">Minor Correlation</rdfs:label>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/narrowAlignment">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:label xml:lang="en-US">Narrow Alignment</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:comment xml:lang="en-US">The competency being described covers all of the relevant concepts in the target competency as well as relevant concepts not found in the target competency.</rdfs:comment>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/narrowCorrelation">
    <rdfs:label xml:lang="en-US">Narrow Correlation</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:comment xml:lang="en-US">The learning resource being described covers all of the relevant concepts in the competency statement as well as relevant concepts not found in the competency.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/conformsTo"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/LearningResource"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/prerequisiteAlignment">
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:comment xml:lang="en-US">The competency being described is a prerequisite to the target competency.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:label xml:lang="en-US">Prerequisite Alignment</rdfs:label>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/prerequisiteCompetency">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/conformsTo"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/LearningResource"/>
    <rdfs:comment xml:lang="en-US">A prerequisite competency for the learning resource being described.</rdfs:comment>
    <rdfs:label xml:lang="en-US">Prerequisite Competency</rdfs:label>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/proficiencyLevel">
    <schema:domainIncludes rdf:resource="http://purl.org/ASN/schema/core/LearningResource"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <schema:domainIncludes rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/ProficiencyScale"/>
    <dcterms:description xml:lang="en">The value of proficiencyLevel should be a concept IRI of a member of the class ProficiencyScale.</dcterms:description>
    <rdfs:label xml:lang="en-US">Proficiency Level</rdfs:label>
    <rdfs:comment xml:lang="en">The expected performance level of a learner or professional in relationship to a competency.</rdfs:comment>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/publicationStatus">
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:label xml:lang="en-US">Publication Status</rdfs:label>
    <rdfs:comment xml:lang="en-US">The publication status of the of the standards document--e.g., Draft, Published, Deprecated.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/qualityLabel">
    <rdfs:comment xml:lang="en-US">Qualitative description of this degree of achievement used for column headers in tabular rubrics.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/CriterionLevel"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:label xml:lang="en-US">Quality Label</rdfs:label>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/repositoryDate">
    <rdfs:label xml:lang="en-US">Repository Date</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:comment xml:lang="en-US">The date the standards document was added to the repository.</rdfs:comment>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/score">
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/CriterionLevel"/>
    <rdfs:label xml:lang="en-US">Criterion Level Score</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <owl:equivalentProperty rdf:resource="https://ceds.ed.gov/CEDSElementDetails.aspx?TermId=14447"/>
    <rdfs:comment xml:lang="en-US">Points to be awarded for achieving this level for a Rubric Criterion.</rdfs:comment>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/sequence">
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/RubricCriterion"/>
    <rdfs:comment xml:lang="en-US">Numeric value representing the resource's position in the list (array) of resources.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#positiveInteger"/>
    <rdfs:label xml:lang="en-US">Sequence</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/CriterionLevel"/>
    <owl:equivalentProperty rdf:resource="https://ceds.ed.gov/CEDSElementDetails.aspx?TermId=14445"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/skillEmbodied">
    <rdfs:comment xml:lang="en-US">Cognitive, affective, psychomotor or other skill directly or indirectly embodied in the statement.</rdfs:comment>
    <rdfs:label xml:lang="en-US">Skill Embodied</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/source">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:comment xml:lang="en-US">The original resource which this resource is based on or derived from.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Rubric"/>
    <rdfs:label xml:lang="en-US">Source</rdfs:label>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/statementLabel">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:comment xml:lang="en-US">The textual label designating the class of statement in the structure of statements in the Standard Document--e.g., Standard, Benchmark, Strand, Topic, etc.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:label xml:lang="en-US">Statement Label</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/statementNotation">
    <rdfs:comment xml:lang="en-US">An alphanumeric identifier of a Statement as defined by the promulgating agency. Statement notation must be unique within the defining organization's namespace.</rdfs:comment>
    <rdfs:label xml:lang="en-US">Statement Notation</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifer"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/teachesCompetency">
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:label xml:lang="en-US">Teaches Competency</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/LearningResource"/>
    <rdfs:comment xml:lang="en-US">The competency that the learning resource being described teaches.</rdfs:comment>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/weight">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:label xml:lang="en-US">Weight</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:comment xml:lang="en-US">A textually asserted measurement of the weight, degree, percent, or strength of a recommendation, requirement, or comparison; e.g., "15%", ".5".</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
  </rdf:Property>
  
</rdf:RDF>
