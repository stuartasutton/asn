            "rdfs:domain": "asn:ProgressionModel",
            "rdfs:isDefinedBy": "asn:",
            "rdfs:label": {"en-US": "Has Progression Level"},
            "rdfs:range": "asn:ProgressionLevel"
        },
        {
            "@id": "asn:hasProgressionModel",
            "@type": "rdfs:Property",
            "rdfs:comment": {"en-US": "Reference to a progression model used."},
            "rdfs:domain": ["rdfs:Resource", "asn:Rubric"],
            "rdfs:isDefinedBy": "asn:",
            "rdfs:label": {"en-US": "Has Progression Model"},
            "rdfs:range": "asn:ProgressionModel"
        },
        {
            "@id": "asn:hasScope",
            "@type": "rdfs:Property",
            "rdfs:comment": {"en-US": "Description of what the rubric's creator intended to assess or evaluate."},
