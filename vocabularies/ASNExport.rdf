<?xml version="1.0"?><rdf:RDF xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:skos="http://www.w3.org/2004/02/skos/core#">  <skos:ConceptScheme rdf:about="http://purl.org/ASN/scheme/ASNExport/">    <dc:title xml:lang="en-US">ASN Export Version Vocabulary</dc:title>    <dc:description xml:lang="en-US">We distribute and make available through web services only the most current export version of the ASN. The current ASN export version is 3.1.0.</dc:description>    <dcterms:created rdf:datatype="http://purl.org/dc/terms/W3CDTF">2009-07-27</dcterms:created>    <dcterms:modified rdf:datatype="http://purl.org/dc/terms/W3CDTF">2015-01-07</dcterms:modified>    <skos:hasTopConcept rdf:resource="http://purl.org/ASN/scheme/ASNExport/1.3.0"/>    <skos:hasTopConcept rdf:resource="http://purl.org/ASN/scheme/ASNExport/1.4.0"/>    <skos:hasTopConcept rdf:resource="http://purl.org/ASN/scheme/ASNExport/1.4.1"/>    <skos:hasTopConcept rdf:resource="http://purl.org/ASN/scheme/ASNExport/2.0.0"/>    <skos:hasTopConcept rdf:resource="http://purl.org/ASN/scheme/ASNExport/3.0.0"/>    <skos:hasTopConcept rdf:resource="http://purl.org/ASN/scheme/ASNExport/3.1.0"/>  </skos:ConceptScheme>  <skos:Concept rdf:about="http://purl.org/ASN/scheme/ASNExport/1.3.0">    <skos:inScheme rdf:resource="http://purl.org/ASN/scheme/ASNExport/"/>    <skos:prefLabel xml:lang="en-US">Export: 1.3.0</skos:prefLabel>    <skos:scopeNote xml:lang="en-US">RDF/XML export to include {{asn:statementNotation}} (state derived coding reference) where present.</skos:scopeNote>    <skos:topConceptOf rdf:resource="http://purl.org/ASN/scheme/ASNExport/" />  </skos:Concept>  <skos:Concept rdf:about="http://purl.org/ASN/scheme/ASNExport/1.4.0">    <skos:inScheme rdf:resource="http://purl.org/ASN/scheme/ASNExport/"/>    <skos:prefLabel xml:lang="en-US">Export: 1.4.0</skos:prefLabel>    <skos:scopeNote xml:lang="en-US">RDF/XML exported file names to contain the first 35 characters of a document's title. RDF/XML exports to include data elements: {{asn:publicationStatus}} with the value of "Published", {{asn:authorityStatus}} with the value of "Original", and {{asn:indexingStatus}} with the value of "Yes".</skos:scopeNote>    <skos:topConceptOf rdf:resource="http://purl.org/ASN/scheme/ASNExport/" />  </skos:Concept>  <skos:Concept rdf:about="http://purl.org/ASN/scheme/ASNExport/1.4.1">    <skos:inScheme rdf:resource="http://purl.org/ASN/scheme/ASNExport/"/>    <skos:prefLabel xml:lang="en-US">Export: 1.4.1</skos:prefLabel>    <skos:scopeNote xml:lang="en-US">RDF/XML export to include {{asn:localSubject}} where present. Expanded the ASN repository to include standards from subjects "Social Studies" and "The Arts" for all 50 U.S. states.</skos:scopeNote>    <skos:topConceptOf rdf:resource="http://purl.org/ASN/scheme/ASNExport/" />  </skos:Concept>  <skos:Concept rdf:about="http://purl.org/ASN/scheme/ASNExport/2.0.0">    <skos:inScheme rdf:resource="http://purl.org/ASN/scheme/ASNExport/"/>    <skos:prefLabel xml:lang="en-US">Export: 2.0.0</skos:prefLabel>    <skos:scopeNote xml:lang="en-US">RDF/XML export to include {{dcterms:license}} data element.</skos:scopeNote>    <skos:topConceptOf rdf:resource="http://purl.org/ASN/scheme/ASNExport/" />  </skos:Concept>  <skos:Concept rdf:about="http://purl.org/ASN/scheme/ASNExport/3.0.0">    <skos:inScheme rdf:resource="http://purl.org/ASN/scheme/ASNExport/"/>    <skos:prefLabel xml:lang="en-US">Export: 3.0.0</skos:prefLabel>    <skos:scopeNote xml:lang="en-US">Export 3.0.0 includes: {{dcterms:tableOfContents}} data element which links to that documents manifest file (statement order/easy parsing). New {{asn:identifier}} data element contains previous PURL URI if previously assigned. New asn.jesandco.org URI's which enable content negotiation. XML:lang attributes are now included on every literal value. {{dcterms:license}} and {{asn:exportVersion}} are now non-literals. {{dcterms:created}} data element changed to {{dcterms:dateValid}}. {{dc:subject}} data element changed to {{dcterms:subject}}. {{dc:description}} data element changed to {{dcterms:description}}. {{dc:language}} data element changed to {{dcterms:language}} and is now a non-literal value. Removed all {{rdf:Seq}} and {{rdf:li}} resource groupings and replaced with {{gemq:hasChild}} data element (for statement ordering see manifest URI).</skos:scopeNote>    <skos:topConceptOf rdf:resource="http://purl.org/ASN/scheme/ASNExport/" />  </skos:Concept>    <skos:Concept rdf:about="http://purl.org/ASN/scheme/ASNExport/3.1.0">    <skos:inScheme rdf:resource="http://purl.org/ASN/scheme/ASNExport/"/>    <skos:prefLabel xml:lang="en-US">Export: 3.1.0</skos:prefLabel>    <skos:scopeNote xml:lang="en-US">Export 3.1.0 includes: Removed {{dcterms:license}} from all statements and documents. Added licensing and rights information as a comment.  Added an explicit RDF description of the Information Resource.</skos:scopeNote>    <skos:topConceptOf rdf:resource="http://purl.org/ASN/scheme/ASNExport/" />  </skos:Concept></rdf:RDF>