  <!-- ASN Pre-Kindergarten Definition -->
  <skos:Concept rdf:about="http://purl.org/ASN/scheme/ASNEducationLevel/Pre-K">
    <skos:prefLabel xml:lang="en-US">Pre-Kindergarten</skos:prefLabel>
    <skos:altLabel xml:lang="en-US">Nursery School</skos:altLabel>
    <skos:altLabel xml:lang="en-US">Pre-K</skos:altLabel>
    <skos:altLabel xml:lang="en-US">Preschool</skos:altLabel>
    <skos:broader rdf:resource="http://purl.org/ASN/scheme/ASNEducationLevel/ElementarySchool"/>
    <skos:scopeNote xml:lang="en-US">Activities and/or experiences that are intended to effect developmental changes in children, from birth to entrance in kindergarten</skos:scopeNote>
    <skos:inScheme rdf:resource="http://purl.org/ASN/scheme/ASNEducationLevel/" />
  </skos:Concept>
