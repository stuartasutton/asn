<!-- ====================================================== -->
<!-- ASN APPLICATION PROFILE DESCRIPTION                    -->
<!-- ====================================================== -->    

  <rdf:Property rdf:about="http://purl.org/ASN/schema/ap/">
    <dct:title xml:lang="en-US">The Achievement Standards Network Application Profile</dct:title>
    <dct:description xml:lang="en-US">The Achievement Standards Network Application Profile (ASN-AP) provides URIs and definitions for all of the terms used in ASN metadata instances including properties from the ASN namespace as well as properties borrowed from other namespaces such as those of the Dublin Core Metadata Initiatve (DCMI), Simple Knowledge Organization System (SKOS), Creative Commons and Schema.org.</dct:description>
    <dct:publisher xml:lang="en-US">University of Washington</dct:publisher>
    <dct:contributor xml:lang="en-US">D2L</dct:contributor>
    <dct:language xml:lang="en-US">English</dct:language>
    <dct:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2020-11-28</dct:issued>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2023-01-02</dct:modified>
  </rdf:Property>
  
<!-- ====================================================== -->
<!-- CLASSES                                                -->
<!-- ====================================================== -->
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/CompetencyMap">
    <rdfs:comment xml:lang="en-US">A Competency Map can be viewed as an identified graph that aggregates two or more competency statements (asn:statement) and the relationships asserted among them.</rdfs:comment>
    <rdfs:label xml:lang="en-US">CompetencyMap</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <dc:description>A Competency Map is a profile that borrows statements (asn:Statement) from two or more standard documents (asn:StandardDocument).A Competency Map may or may not define new statements in its own namespace.</dc:description>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/CorrelationResource">
    <rdfs:comment xml:lang="en-US">A resource that describes the mapping between a learning resource and a node in a learning outcomes framework.</rdfs:comment>
    <rdfs:label xml:lang="en-US">Correlation Resource</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <dc:description>The CorrelationResource is an entity intended to provide a rich description of a mapping (correlation) between a LearningResource and a Statement when a binary assertion of a relationship is insufficient.</dc:description>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/CriterionCategory">
    <rdfs:label xml:lang="en-US">Criterion Category</rdfs:label>
    <rdfs:comment xml:lang="en-US">Resource identifying a category by which Rubric Criterion may be grouped.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdfs:Class>  
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/CriterionLevel">
    <rdfs:label xml:lang="en-US">Criterion Level</rdfs:label>
    <rdfs:comment xml:lang="en-US">Resource identifying one or more benchmarks that must be met to achieve a performance task.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/DerivedStandardDocument">
    <rdfs:label xml:lang="en-US">Derived Standard Document</rdfs:label>
    <rdfs:comment xml:lang="en-US">A collection of statements representing an expectation of what students should know, value, and be able to do at a target age or age range that are substantially defined by a different jurisdiction.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/DerivedStatement">
    <rdfs:label xml:lang="en-US">Derived Statement</rdfs:label>
    <rdfs:comment xml:lang="en-US">An assertion representing an expectation of what students should know, value, and be able to do at a target age or age range that is derived from a statement defined by a different jurisdiction.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/EducationalFramework">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:label xml:lang="en-US">Educational Framework</rdfs:label>
    <dc:description xml:lang="en-US">Similar to the class skos:ConceptScheme with an intended focus on education and training. Examples of educational frameworks include, but are not limited to, frameworks enumerating competencies, text complexity, proficiency scales, assessment rubrics of various forms, and specialized subject vocabularies.</dc:description>
    <rdfs:comment xml:lang="en-US">The class of all structured sets of conceptual resources intentionally designed for use as value vocabulary terms in education and training contexts.</rdfs:comment>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/JurisdictionScheme">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:label xml:lang="en-US">Jurisdiction Encoding Schemes</rdfs:label>
    <rdfs:comment xml:lang="en-US">Controlled vocabularies of terms denoting the juristiction or domain of the standards document.</rdfs:comment>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/LearningResource">
    <rdfs:comment xml:lang="en-US">A resource that is used as part of an Educational Activity (e.g. a textbook) or that describes (e.g. a lesson plan) or records the Educational Activity (e.g. an audio- or video-recording of a lesson).</rdfs:comment>
    <rdfs:label xml:lang="en-US">Learning Resource</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/ProgressionLevel">
    <rdfs:label xml:lang="en-US">Progression Level</rdfs:label>
    <rdfs:comment xml:lang="en-US">Identifiable point along a developmental progression of competence, achievement or temporal position.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/ProgressionModel">
    <rdfs:comment xml:lang="en-US">Model of identifiable points along a developmental progression including increasing levels of competence, achievement or temporal position (e.g., "Second Quarter").</rdfs:comment>
    <rdfs:label xml:lang="en-US">Progression Model</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/ProficiencyScale">
    <rdfs:label xml:lang="en-US">Proficiency Scale</rdfs:label>
    <rdfs:comment xml:lang="en-US">The class of all structured concept schemes identifying discrete levels of expertise and performance mastery required either by a competency or to be achieved by a learner.</rdfs:comment>
    <dc:description xml:lang="en-US">Proficiency scales define levels of performance (what a person does) as distinct from knowledge of specific information (what a person knows) and outline tasks a person can manage and the skills necessary to progressively accomplish explicit competencies at increasing levels of complexity. Proficiency scales: (1) assist in making judgments about the kinds of tasks related to a competency that a person is able to perform; and (2) to compare the abilities of different persons with regard to achievement of those competencies at different levels.</dc:description>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/RightsAndAttribution">
    <rdfs:label xml:lang="en-US">Rights and Attribution</rdfs:label>
    <rdfs:comment xml:lang="en-US">Resource providing information about the rights and attribution requirements of the entity making the metadata description available. These rights are distinct from any rights held in the content of the competency framework or competencies described. </rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/Rubric">
    <rdfs:comment xml:lang="en-US">Resource providing guidance in evaluating the quality and level of person's responses or actions based on an explicit set of criteria.</rdfs:comment>
    <rdfs:label xml:lang="en-US">Rubric</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/RubricCriterion">
    <rdfs:label xml:lang="en-US">Rubric Criterion</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:comment xml:lang="en-US">Resource identifying a criterion that must be met to demonstrate quality performance of a objective or task.</rdfs:comment>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/StandardDocument">
    <rdfs:label xml:lang="en-US">Standard Document</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:comment xml:lang="en-US">A collection of statements representing an expectation of what students should know, value, and be able to do at a target age or age range.</rdfs:comment>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/Statement">
    <rdfs:label xml:lang="en-US">Statement</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:comment xml:lang="en-US">An assertion representing an expectation of what students should know, value, and be able to do at a target age or age range.</rdfs:comment>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="http://purl.org/ASN/schema/core/StatusScheme">
    <rdfs:comment xml:lang="en-US">Controlled vocabularies of terms denoting the publication status of the standards document.</rdfs:comment>
    <rdfs:label xml:lang="en-US">Status Encoding Schemes</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdfs:Class>

<!-- ====================================================== -->
<!-- PROPERTIES                                             -->
<!-- ====================================================== -->
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/alignFrom">
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <dc:description xml:lang="en-US">An alignment is an assertion of some degree of eqiuvalency between the subject and the object of the assertion.</dc:description>
    <rdfs:comment>A standards document or statement from which the statement or document being described is aligned.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:label>align from</rdfs:label>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/alignTo">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:label>align to</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <dc:description xml:lang="en-US">An alignment is an assertion of some degree of eqiuvalency between the subject and the object of the assertion.</dc:description>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:comment>A standards document or statement to which the statement or document being described is aligned.</rdfs:comment>
  </rdf:Property>
   
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/altIdentifier">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
    <rdfs:label>Alternative Identifier</rdfs:label>
    <rdfs:comment>An alternative URI by which the resource is identified.</rdfs:comment>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://www.w3.org/2004/02/skos/core#altLabel">
    <rdfs:label>Alternative Label</rdfs:label>
    <rdfs:comment>An alternative lexical label for a resource.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2004/02/skos/core#"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/altStatementNotation">
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:label>Alternative Statement Notation</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:comment>An alphanumeric notation or ID code identifying the statement in common use among end-users.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifer"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/assessesCompetency">
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/LearningResource"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:label>Assesses Competency</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:comment>The competency that the learning resource being described assesses.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/conformsTo"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/assessmentRubric">
    <rdfs:comment xml:lang="en-US">Related resource providing guidance in evaluating the quality and level of person's responses or actions based on an explicit set of criteria.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:label xml:lang="en-US">Assessment Rubric</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Rubric"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://creativecommons.org/ns#attributionName">
    <rdfs:comment xml:lang="en-US">The name the creator of a Work would like used when attributing re-use. In this profile, the attribution name is of the name of the owner, publisher or registry to which attribution is owed.</rdfs:comment>
    <dct:description xml:lang="en-US">Frequently used provide attribution to the creator of the data representation of a standards document.</dct:description>
    <rdfs:isDefinedBy rdf:resource="http://creativecommons.org/ns#"/>
    <rdfs:label xml:lang="en-US">Attribution Name</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/RightsAndAttribution"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://creativecommons.org/ns#attributionURL">
    <rdfs:isDefinedBy rdf:resource="http://creativecommons.org/ns#"/>
    <rdfs:comment xml:lang="en-US">The URL the creator of a Work would like used when attributing re-use.In this profile, the attribution URL is of the URL the owner, publisher or egistry to which attribution is owed would like used.</rdfs:comment>
    <rdfs:label xml:lang="en-US">Attribution URL</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/RightsAndAttribution"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://id.loc.gov/vocabulary/relators/aut">
    <rdfs:comment>A person, family, or organization responsible for creating a work.</rdfs:comment>
    <rdfs:label>Author</rdfs:label>
    <dc:description>In ASN, the author property should be used only where individual people have been identified as authors.</dc:description>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/elements/1.1/"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/authorityStatus">
    <rdfs:label>Authority Status</rdfs:label>
    <rdfs:comment>The status of a standards statement denoting whether the statement owes its origin to the promulgating agency of the standards document of which the statement is a member (i.e., value "Original") or to some 3rd party (i.e., "Derived").</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/benchmark">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/CriterionLevel"/>
    <rdfs:comment xml:lang="en-US">Description of a level of achievement on a performance task defined by the RubricCriterion.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"/>
    <vann:usageNote xml:lang="en-US">Can have one or more per CriterionLevel.</vann:usageNote>
    <rdfs:label xml:lang="en-US">Benchmark</rdfs:label>
    <owl:equivalentProperty rdf:resource="https://ceds.ed.gov/CEDSElementDetails.aspx?TermId=14443"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://www.w3.org/2004/02/skos/core#broader"> 
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2004/02/skos/"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:comment xml:lang="en-US">Relates a concept to a concept that is more general in meaning.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <vann:usageNote xml:lang="en-US">Broader concepts are typically rendered as parents in a concept hierarchy (tree).</vann:usageNote>
    <rdfs:label xml:lang="en-US">Broader</rdfs:label>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/broadAlignment">
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:label>Broad Alignment</rdfs:label>
    <rdfs:comment>The target competency covers all of the relevant concepts in the competency being described as well as relevant concepts not found in the competency being described.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/broadAlignment">
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:label>Broad Alignment</rdfs:label>
    <rdfs:comment>The target competency covers all of the relevant concepts in the competency being described as well as relevant concepts not found in the competency being described.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/broadCorrelation">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:comment>The competency covers all of the relevant concepts in the learning resource being described as well as relevant concepts not found in the learning resource.</rdfs:comment>
    <rdfs:label>Broad Correlation</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/LearningResource"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/conformsTo"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://www.w3.org/2004/02/skos/core#broader">
    <rdfs:label>Broader</rdfs:label>
    <rdfs:comment>Relates a concept to a concept that is more general in meaning.</rdfs:comment>
    <dct:description>Broader concepts are typically rendered as parents in a concept hierarchy (tree).</dct:description>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2004/02/skos/core#"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/comment">
    <rdfs:comment>Supplemental text provided by the promulgating body that clarifies the nature, scope or use of the statement being described.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:label>comment</rdfs:label>
    <dc:description xml:lang="en-US">Use comment when the text provides useful context for the statement.</dc:description>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/complexityLevel">
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/ProficiencyScale"/>
    <rdfs:comment>The expected performance level of a learner or professional as defined by a competency.</rdfs:comment>
    <dc:description xml:lang="en-US">The value of asn:complexityLevel should be a concept IRI of a member of the class asn:ProficiencyScale.</dc:description>
    <rdfs:label>Complexity Level</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/ASN/schema/core/proficiencyLevel"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/comprisedOf">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:comment xml:lang="en-US">The resource being described includes, comprehends or encompass, in whole or in part, the meaning, nature or importance of the resource being referenced.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:label>Comprised Of</rdfs:label>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/conceptKeyword">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Literal"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:comment>A word or phrase used by the promulgating agency to refine and differentiate individual statements contextually.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/elements/1.1/subject"/>
    <rdfs:label>Concept Keyword</rdfs:label>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/conceptTerm">
    <rdfs:label>Concept Term</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/elements/1.1/subject"/>
    <rdfs:comment>A term from a controlled vocabulary used by the promulgating agency to refine and differentiate individual statements contextually.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/dc/terms/created">
    <rdfs:comment>Date of creation of the resource.</rdfs:comment>
    <rdfs:label>Date Created</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/criterionCategoryOf">
    <rdfs:label xml:lang="en-US">Criterion Category Of</rdfs:label>
    <rdfs:comment xml:lang="en-US">Resource referenced is a Rubric to which this CriterionCategory belongs.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/CriterionCategory"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Rubric"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/criterionFor">
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/RubricCriterion"/>
    <rdfs:label xml:lang="en-US">Criterion For</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Rubric"/>
    <rdfs:comment xml:lang="en-US">Reference to the asn:Rubric to which the asn:RubricCriterion being described belongs.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/crossSubjectReference">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/elements/1.1/references"/>
    <rdfs:label>Cross-subject Reference</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:comment>An explicit reference by the promulgating agency establishing a relationship between the statement being described and statement in a separate subject standards document (interdisciplinary statement references).</rdfs:comment>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/dc/terms/dateCopyrighted">
    <rdfs:comment>Typically a year. Recommended practice is to describe the date, date/time, or period of time as recommended for the property Date, of which this is a subproperty.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:label>Date Copyrighted</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
    <rdfs:range rdf:resource="http://purl.org/dc/terms/W3CDTF"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/derivedFrom">
    <rdfs:comment>The URI of a statement from which the derived statement being described has been derived.</rdfs:comment>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:label>Derived From</rdfs:label>
    <dc:description xml:lang="en-US">The derivedFrom property is used by 3rd parties only in derived statements and must appear in conjunction with a value of "Derived" in the statement's authorityStatus property.</dc:description>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/dc/terms/description">
    <rdfs:comment>An account of the resource.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
    <rdfs:label>Description</rdfs:label>
    <dc:description xml:lang="en-US">In this ASN profile, the description property is used to provide the actual text of the competency (Statement) and to provide an abstract of the competency framework (StandardDocument).</dc:description>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/dc/terms/description">
    <rdfs:comment>An account of the resource.</rdfs:comment>
    <dc:description xml:lang="en-US">In this ASN profile, the description property is used to provide the actual text of the competency (Statement) and to provide an abstract of the competency framework (StandardDocument).</dc:description>
    <rdfs:label>Description</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/dc/terms/educationLevel">
    <rdfs:label>Education Level</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:comment>A class of agents, defined in terms of progression through an educational or training context, for which the described resource is intended.</rdfs:comment>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/exactAlignment">
    <rdfs:comment>The relevant concepts in both competencies are coextensive.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:label>Exact Alignment</rdfs:label>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/exactCorrelation">
    <rdfs:comment>The relevant concepts in the educational resource and the standard statement are coextensive.</rdfs:comment>
    <rdfs:label>Exact Correlation</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/LearningResource"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/conformsTo"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/exportVersion">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:label>Export Version</rdfs:label>
    <rdfs:comment>The URI of the export version of the standards file.</rdfs:comment>
  </rdf:Property>  
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/hasChild">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:label>Has Child</rdfs:label>
    <rdfs:comment>The resource being described is higher in some arbitrary hierarchy than the resource being pointed to by this Relation element.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/hasCriterion">
    <vann:usageNote xml:lang="en-US">Both asn:RubricCriterion and CriterionCategory have an asn:sequence property to identify node order; however, json-ld encodings should use @list on the asn:hasCriterion and asn:hasLevel properties to maintain sequencing of RubricCriterion.</vann:usageNote>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/RubricCriterion"/>
    <rdfs:comment xml:lang="en-US">RubricCriterion referenced by the Rubric that defining criterion to be met to demonstrate quality in execution in performance of a task or obtaining an objective.</rdfs:comment>
    <rdfs:label xml:lang="en-US">Has Criterion</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Rubric"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/hasCriterionCategory">
    <vann:usageNote xml:lang="en-US">Large rubrics are occasionally divided into logical sections or categories that need to be identified and described.</vann:usageNote>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/CriterionCategory"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Rubric"/>
    <rdfs:comment xml:lang="en-US">Resource referenced by the Rubric that defines categories for clustering logical sets of RubricCriterion.</rdfs:comment>
    <rdfs:label xml:lang="en-US">Has Criterion Category</rdfs:label>
  </rdf:Property>
 
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/hasLevel">
    <vann:usageNote xml:lang="en-US">Both RubricCriterion and CriterionCategory have an asn:sequence property to identify node order; however, json-ld encodings should use @list on the asn:hasCriterion and asn:hasLevel properties to maintain such sequencing.</vann:usageNote>
    <rdfs:label xml:lang="en-US">Has Level</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/RubricCriterion"/>
    <owl:equivalentProperty rdf:resource="https://ceds.ed.gov/CEDSElementDetails.aspx?TermId=14443"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/CriterionLevel"/>
    <rdfs:comment xml:lang="en-US">Resource description of a level of performance based on a RubricCriterion.</rdfs:comment>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/dc/terms/hasPart">
    <dc:description xml:lang="en-US">This property is intended to be used with non-literal values. This property is an inverse property of Is Part Of.</dc:description>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
    <rdfs:label>Has Part</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:comment>A related resource that is included either physically or logically in the described resource.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/hasProgressionLevel">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:label xml:lang="en-US">Has Progression Level</rdfs:label>
    <rdfs:comment xml:lang="en-US">Reference to a specific point in a progression model.</rdfs:comment>
    <rdfs:domain rdf:resource="asn:ProgressionModel"/>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/hasProgressionModel">
    <rdfs:comment xml:lang="en-US">Reference to a progression model used.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:label xml:lang="en-US">Has Progression Model</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/ProgressionModel"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/hasScope">
    <rdfs:comment xml:lang="en-US">Description of what the rubric's creator intended to assess or evaluate.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Rubric"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:label xml:lang="en-US">Has Scope</rdfs:label>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://www.w3.org/2004/02/skos/core#hasTopConcept">
    <rdfs:label xml:lang="en-US">Has Top Concept</rdfs:label>
    <rdfs:comment xml:lang="en-US">Relates, by convention, a concept scheme to a concept which is topmost in the broader/narrower concept hierarchies for that scheme, providing an entry point to these hierarchies.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2004/02/skos/core#"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/dc/terms/hasVersion">
    <dc:description xml:lang="en-US">Changes in version imply substantive changes in content rather than differences in format. This property is intended to be used with non-literal values. This property is an inverse property of Is Version Of.</dc:description>
    <rdfs:label>Has Version</rdfs:label>
    <rdfs:comment>A related resource that is a version, edition, or adaptation of the described resource.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/indexingStatus">
    <rdfs:comment>The status of the statement denoting whether correlators should or should not assign the statement during correlation--Values="Yes"/"No".</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:label>Indexing Status</rdfs:label>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://www.w3.org/2004/02/skos/core#inScheme">
    <rdfs:comment>Relates a resource (for example a concept) to a concept scheme in which it is included.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2004/02/skos/core#"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:label>Is In Scheme</rdfs:label>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/isChildOf">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:comment>The statement being described is lower in some arbitrary hierarchy than the statement identified in the asn:isChildOf property. The statement identified is a parent of the statement being described.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:label>Statement Label</rdfs:label>
    <rdfs:range rdf:resource= "http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:range rdf:resource= "http://purl.org/ASN/schema/core/Statement"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/dc/terms/isPartOf">
    <rdfs:comment>A related resource in which the described resource is physically or logically included.</rdfs:comment>
    <dc:description xml:lang="en-US">This property is intended to be used with non-literal values. This property is an inverse property of Has Part.</dc:description>
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:label>Is Part Of</rdfs:label>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/dc/terms/isVersionOf">
    <rdfs:comment>A related resource of which the described resource is a version, edition, or adaptation.</rdfs:comment>
    <rdfs:label>Is Version Of</rdfs:label>
    <dc:description xml:lang="en-US">Changes in version imply substantive changes in content rather than differences in format. This property is intended to be used with non-literal values. This property is an inverse property of Has Version.</dc:description>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/jurisdiction">
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <dc:description xml:lang="en-US">The URI of the jurisdiction from the ASNJurisdiction controlled vocabulary.</dc:description>
    <rdfs:label>Jurisdiction</rdfs:label>
    <rdfs:comment>The jurisdiction or domain of the standard's application--e.g., California, Nebraska, etc.</rdfs:comment>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/dc/elements/1.1/language">
    <rdfs:label>Language</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:comment>A language of the resource.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/elements/1.1/"/>
    <dc:description xml:lang="en-US">Recommended practice is to use either a non-literal value representing a language from a controlled vocabulary such as ISO 639-2 or ISO 639-3, or a literal value consisting of an IETF Best Current Practice 47 [IETF-BCP47] language tag</dc:description>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/levelFor">
    <rdfs:comment xml:lang="en-US">Reference to the asn:RubricCriterion to which the asn:CriterionLevel being described belongs.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:label xml:lang="en-US">Level For</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/RubricCriterion"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/CriterionLevel"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/dc/terms/license">
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:range rdf:resource="http://purl.org/dc/terms/LicenseDocument"/>
    <dc:description xml:lang="en-US">Recommended practice is to identify the license document with a URI. If this is not possible or feasible, a literal value that identifies the license may be provided.</dc:description>
    <rdfs:label>License</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
    <rdfs:comment>A legal document giving official permission to do something with the resource.</rdfs:comment>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/listID">
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifer"/>
    <rdfs:comment>A set of one or more alphanumeric characters and/or symbols denoting the positioning of the statement being described in a sequential listing of statements.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:label>List ID</rdfs:label>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/localSubject">
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:comment>The text string denoting the subject of the statement as designated by the promulgating agency. This subject may be different from the asn:subject used for subject classification within the ASN.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"/>
    <rdfs:label>Local Subject</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/subject"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/majorAlignment">
    <rdfs:comment>Major overlap of relevant concepts between the two competencies.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:label>Major Alignment</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/majorCorrelation">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/conformsTo"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/LearningResource"/>
    <rdfs:comment>Major overlap of relevant concepts between the learning resource being described and the competency statement.</rdfs:comment>
    <rdfs:label>Major Correlation</rdfs:label>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/minorAlignment">
    <rdfs:comment>Minor overlap of relevant concepts between the two competencies.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:label>Minor Alignment</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/minorCorrelation">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/conformsTo"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/LearningResource"/>
    <rdfs:comment>Minor overlap of relevant concepts between the learning resource being described and the competency statement.</rdfs:comment>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:label>Minor Correlation</rdfs:label>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/dc/terms/modified">
    <rdfs:label>Date Modified</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/dc/terms/W3CDTF"/>
    <rdfs:comment>Date on which the resource was changed.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/LearningResource"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/RightsAndAttribution"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/narrowAlignment">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:label>Narrow Alignment</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:comment>The competency being described covers all of the relevant concepts in the target competency as well as relevant concepts not found in the target competency.</rdfs:comment>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/narrowCorrelation">
    <rdfs:label>Narrow Correlation</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:comment>The learning resource being described covers all of the relevant concepts in the competency statement as well as relevant concepts not found in the competency.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/conformsTo"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/LearningResource"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://www.w3.org/2004/02/skos/core#narrower">
    <rdfs:label>Narrower</rdfs:label>
    <rdfs:comment>Relates a concept to a concept that is more specific in meaning.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2004/02/skos/core#"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://www.w3.org/2004/02/skos/core#note">
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:label>Note</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2004/02/skos/core#"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:comment>A general note, for any purpose.</rdfs:comment>
    <rdfs:domain rdf:resource="StandardDocument"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://www.w3.org/2004/02/skos/core#prefLabel">
    <rdfs:label>Preferred Label</rdfs:label>
    <rdfs:comment>Preferred LabelThe preferred lexical label for a resource, in a given language."@en</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2004/02/skos/core#"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/prerequisiteAlignment">
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:comment>The competency being described is a prerequisite to the target competency.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:label>Prerequisite Alignment</rdfs:label>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/prerequisiteCompetency">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/conformsTo"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/LearningResource"/>
    <rdfs:comment>A prerequisite competency for the learning resource being described.</rdfs:comment>
    <rdfs:label>Prerequisite Competency</rdfs:label>
  </rdf:Property>

  <rdf:Property rdf:about="http://xmlns.com/foaf/0.1/primaryTopic">
    <rdfs:isDefinedBy rdf:resource="http://xmlns.com/foaf/0.1/"/>
    <rdfs:comment xml:lang="en">The primaryTopic property relates a document to the main thing that the document is about.</rdfs:comment>
    <rdfs:label>Primary Topic</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/RightsAndAttribution"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/proficiencyLevel">
    <schema:domainIncludes rdf:resource="http://purl.org/ASN/schema/core/LearningResource"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <schema:domainIncludes rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/ProficiencyScale"/>
    <dc:description xml:lang="en">The value of asn:proficiencyLevel should be a concept IRI of a member of the class asn:ProficiencyScale.</dc:description>
    <rdfs:label>Proficiency Level</rdfs:label>
    <rdfs:comment xml:lang="en">The expected performance level of a learner or professional in relationship to a competency.</rdfs:comment>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/publicationStatus">
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:label>Publication Status</rdfs:label>
    <rdfs:comment>The publication status of the of the standards document--e.g., Draft, Published, Deprecated.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/dc/elements/1.1/publisher">
    <dc:description xml:lang="en-US">Examples of a Publisher include a person, an organization, or a service. Typically, the name of a Publisher should be used to indicate the entity.</dc:description>
    <rdfs:label>Publisher</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:comment>An entity responsible for making the resource available.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/elements/1.1/"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/qualityLabel">
    <rdfs:comment xml:lang="en-US">Qualitative description of this degree of achievement used for column headers in tabular rubrics.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/CriterionLevel"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:label xml:lang="en-US">Quality Label</rdfs:label>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/repositoryDate">
    <rdfs:label>Repository Date</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
    <rdfs:range rdf:resource="http://purl.org/dc/terms/W3CDTF"/>
    <rdfs:comment>The date the standards document was added to the repository.</rdfs:comment>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/dc/terms/rights">
    <dc:description xml:lang="en-US">Typically, rights information includes a statement about various property rights associated with the resource, including intellectual property rights. Recommended practice is to refer to a rights statement with a URI. If this is not possible or feasible, a literal value (name, label, or short text) may be provided.</dc:description>
    <rdfs:label>Rights</rdfs:label>
    <rdfs:comment>Information about rights held in and over the resource.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/RightsAndAttribution"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/dc/terms/rightsHolder">
    <dc:description xml:lang="en-US">Recommended practice is to refer to the rights holder with a URI. If this is not possible or feasible, a literal value that identifies the rights holder may be provided.</dc:description>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/RightsAndAttribution"/>
    <rdfs:comment>A person or organization owning or managing rights over the resource.</rdfs:comment>
    <rdfs:label>Rights Holder</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/dc/terms/Agent"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://www.w3.org/2004/02/skos/core#scopeNote">
    <rdfs:label>Scope Note</rdfs:label>
    <rdfs:comment>A note that helps to clarify the meaning and/or the use of a concept.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2004/02/skos/core#"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
  </rdf:Property> 
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/score">
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/CriterionLevel"/>
    <rdfs:label xml:lang="en-US">Criterion Level Score</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <owl:equivalentProperty rdf:resource="https://ceds.ed.gov/CEDSElementDetails.aspx?TermId=14447"/>
    <rdfs:comment xml:lang="en-US">Points to be awarded for achieving this level for a RubricCriterion.</rdfs:comment>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/sequence">
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/RubricCriterion"/>
    <rdfs:comment xml:lang="en-US">Numeric value representing the resource's position in the list (array) of resources.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#positiveInteger"/>
    <rdfs:label xml:lang="en-US">Sequence</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/CriterionLevel"/>
    <owl:equivalentProperty rdf:resource="https://ceds.ed.gov/CEDSElementDetails.aspx?TermId=14445"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/skillEmbodied">
    <rdfs:comment>Cognitive, affective, psychomotor or other skill directly or indirectly embodied in the statement.</rdfs:comment>
    <rdfs:label>Skill Embodied</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/source">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:comment xml:lang="en-US">The original resource which this resource is based on or derived from.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Rubric"/>
    <rdfs:label xml:lang="en-US">Source</rdfs:label>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/dc/terms/spatial">
    <rdfs:range rdf:resource="http://purl.org/dc/terms/Location"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
    <rdfs:comment>Spatial characteristics of the resource.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:label>Spatial</rdfs:label>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/statementLabel">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:comment>The textual label designating the class of statement in the structure of statements in the Standard Document--e.g., Standard, Benchmark, Strand, Topic, etc.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:label>Statement Label</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/statementNotation">
    <rdfs:comment>An alphanumeric identifier of a Statement as defined by the promulgating agency. Statement notation must be unique within the defining organization's namespace.</rdfs:comment>
    <rdfs:label>Statement Notation</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifer"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/dc/terms/subject">
    <rdfs:label>Subject</rdfs:label>
    <rdfs:comment>A topic of the resource.</rdfs:comment>
    <skos:note xml:lang="en-US">Used in ASN to denote a subject subject area of the framework; e.g., "science", "mathematics", "English".</skos:note>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/dc/terms/tableOfContents">
    <rdfs:comment>A list of subunits of the resource.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:label>Table of Contents</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/teachesCompetency">
    <rdfs:range rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:label>Teaches Competency</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/conformsTo"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/LearningResource"/>
    <rdfs:comment>The competency that the learning resource being described teaches.</rdfs:comment>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/dc/terms/temporal">
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
    <rdfs:label>Temporal</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:range rdf:resource="http://purl.org/dc/terms/PeriodOfTime"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
    <rdfs:comment>Temporal characteristics of the resource.</rdfs:comment>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/dc/elements/1.1/title">
    <rdfs:label>Title</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/elements/1.1/"/>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:comment>A name given to the resource.</rdfs:comment>
  </rdf:Property>

  <rdf:Property rdf:about="http://purl.org/dc/terms/valid">
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/StandardDocument"/>
    <rdfs:comment>Date (often a range) of validity of a resource.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
    <rdfs:label>Date Valid</rdfs:label>
  </rdf:Property>
  
  <rdf:Property rdf:about="http://purl.org/ASN/schema/core/weight">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ASN/schema/core/"/>
    <rdfs:label>Weight</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:comment>A textually asserted measurement of the weight, degree, percent, or strength of a recommendation, requirement, or comparison; e.g., "15%", ".5".</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
  </rdf:Property>
  
</rdf:RDF>
